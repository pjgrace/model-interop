<?xml version="1.0"?>
<pattern xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation ="Pattern.xsd">
	<patterndata>
		<data>
			<name>test</name>
			<value>test</value>
		</data>
	</patterndata>
	<architecture>		
		<component>
			<id>client</id>
			<address>api.fixer.io</address>
		</component>
	</architecture>
	<behaviour>		
		<state>
			<label>start</label>
			<type>start</type>
			<transition>
				<to>normal</to>
				<guards>
					<equal>
						<param>HTTP.code</param>
						<value>200</value>
					</equal>
				</guards>
			</transition>
			<transition>
				<to>state</to>
				<guards>
					<equal>
						<param>http.code</param>
						<value>408</value>
					</equal>
				</guards>
			</transition>
		</state>		
		<state>
			<label>normal</label>
			<type>normal</type>
			<transition>
				<to>end</to>
				<guards>
					<equal>
						<param>HTTP.code</param>
						<value>200</value>
					</equal>
				</guards>
			</transition>
		</state>		
		<state>
			<label>end</label>
			<type>end</type>
			<success>false</success>
			<report>null</report>
		</state>		
		<state>
			<label>state</label>
			<type>normal</type>
			<transition>
				<to>timeout</to>
				<guards>
					<equal>
						<param>timeout</param>
						<value>2500</value>
					</equal>
				</guards>
			</transition>
			<transition>
				<to>normal</to>
				<guards>
					<equal>
						<param>HTTP.code</param>
						<value>200</value>
					</equal>
				</guards>
			</transition>
		</state>		
		<state>
			<label>timeout</label>
			<type>end</type>
			<success>false</success>
			<report>null</report>
		</state>
	</behaviour>
</pattern>